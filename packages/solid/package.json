{
  "name": "@jwiedeman/gtm-kit-solid",
  "version": "1.0.0",
  "description": "SolidJS primitives and context for GTM Kit - Google Tag Manager integration.",
  "repository": {
    "type": "git",
    "url": "https://github.com/jwiedeman/GTM-Kit.git",
    "directory": "packages/solid"
  },
  "author": "jwiedeman",
  "keywords": [
    "gtm",
    "google-tag-manager",
    "solidjs",
    "solid",
    "primitives"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "solid": "./dist/index.js",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "lint": "eslint --max-warnings=0 \"src/**/*.{ts,tsx}\"",
    "test": "jest --config ./jest.config.cjs --runInBand --passWithNoTests",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@jwiedeman/gtm-kit": "workspace:*"
  },
  "peerDependencies": {
    "solid-js": "^1.0.0"
  },
  "devDependencies": {
    "@solidjs/testing-library": "^0.8.7",
    "solid-js": "^1.8.17",
    "solid-testing-library": "^0.5.1",
    "tslib": "^2.6.2"
  }
}
