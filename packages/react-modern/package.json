{
  "name": "@jwiedeman/gtm-kit-react",
  "version": "1.0.0",
  "description": "React hooks and provider for GTM Kit - Google Tag Manager integration. Supports React 16.8+.",
  "repository": {
    "type": "git",
    "url": "https://github.com/jwiedeman/GTM-Kit.git",
    "directory": "packages/react-modern"
  },
  "homepage": "https://github.com/jwiedeman/GTM-Kit/tree/main/packages/react-modern#readme",
  "bugs": {
    "url": "https://github.com/jwiedeman/GTM-Kit/issues"
  },
  "author": "jwiedeman",
  "keywords": [
    "gtm",
    "google-tag-manager",
    "react",
    "hooks",
    "provider",
    "context",
    "analytics",
    "tracking"
  ],
  "license": "MIT",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "lint": "eslint --max-warnings=0 \"src/**/*.{ts,tsx}\"",
    "test": "jest --config ./jest.config.cjs --runInBand",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@jwiedeman/gtm-kit": "workspace:*"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0",
    "tslib": "^2.6.2"
  }
}
