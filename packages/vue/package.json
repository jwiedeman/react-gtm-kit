{
  "name": "@jwiedeman/gtm-kit-vue",
  "version": "0.1.0",
  "description": "Vue 3 composables and plugin for GTM Kit - Google Tag Manager integration.",
  "repository": {
    "type": "git",
    "url": "https://github.com/jwiedeman/GTM-Kit.git",
    "directory": "packages/vue"
  },
  "author": "jwiedeman",
  "keywords": [
    "gtm",
    "google-tag-manager",
    "vue",
    "vue3",
    "composables"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "lint": "eslint --max-warnings=0 \"src/**/*.{ts,tsx}\"",
    "test": "jest --config ./jest.config.cjs --runInBand",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@jwiedeman/gtm-kit": "workspace:*"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@vue/test-utils": "^2.4.6",
    "@vue/vue3-jest": "^29.2.6",
    "vue": "^3.4.21",
    "tslib": "^2.6.2"
  }
}
