{
  "name": "@jwiedeman/gtm-kit-astro",
  "version": "1.0.0",
  "description": "Astro components and helpers for GTM Kit - Google Tag Manager integration with View Transitions support.",
  "repository": {
    "type": "git",
    "url": "https://github.com/jwiedeman/GTM-Kit.git",
    "directory": "packages/astro"
  },
  "homepage": "https://github.com/jwiedeman/GTM-Kit/tree/main/packages/astro#readme",
  "bugs": {
    "url": "https://github.com/jwiedeman/GTM-Kit/issues"
  },
  "author": "jwiedeman",
  "keywords": [
    "gtm",
    "google-tag-manager",
    "astro",
    "astrojs",
    "view-transitions",
    "static-site",
    "analytics",
    "tracking"
  ],
  "license": "MIT",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js",
      "require": "./dist/components/index.cjs"
    }
  },
  "files": [
    "dist",
    "src/components/*.astro"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "lint": "eslint --max-warnings=0 \"src/**/*.ts\"",
    "test": "jest --config ./jest.config.cjs --runInBand",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@jwiedeman/gtm-kit": "workspace:*"
  },
  "peerDependencies": {
    "astro": "^3.0.0 || ^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "astro": "^4.16.0",
    "tslib": "^2.6.2"
  }
}
