{
  "name": "@jwiedeman/gtm-kit",
  "version": "0.1.0",
  "description": "Framework-agnostic Google Tag Manager client - the core of GTM Kit.",
  "repository": {
    "type": "git",
    "url": "https://github.com/jwiedeman/GTM-Kit.git",
    "directory": "packages/core"
  },
  "author": "jwiedeman",
  "keywords": [
    "gtm",
    "google-tag-manager",
    "analytics",
    "dataLayer"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "lint": "eslint --max-warnings=0 \"src/**/*.{ts,tsx}\"",
    "test": "jest --config ./jest.config.cjs --runInBand",
    "typecheck": "tsc --noEmit && node scripts/run-tsd.mjs"
  },
  "dependencies": {},
  "devDependencies": {
    "tsd": "^0.31.1",
    "tslib": "^2.6.2"
  },
  "tsd": {
    "directory": "src"
  }
}
