{
  "name": "@react-gtm-kit/cli",
  "version": "0.1.0",
  "description": "CLI tool for easy GTM Kit setup - auto-detects your framework and installs everything you need.",
  "license": "MIT",
  "type": "module",
  "bin": {
    "gtm-kit": "./dist/cli.js",
    "gtm-kit-init": "./dist/cli.js"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "lint": "eslint --max-warnings=0 \"src/**/*.{ts,tsx}\"",
    "test": "jest --config ./jest.config.cjs --runInBand",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {},
  "devDependencies": {
    "tslib": "^2.6.2"
  }
}
