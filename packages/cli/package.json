{
  "name": "@jwiedeman/gtm-kit-cli",
  "version": "1.0.0",
  "description": "CLI tool for easy GTM Kit setup - auto-detects your framework and installs everything you need.",
  "repository": {
    "type": "git",
    "url": "https://github.com/jwiedeman/GTM-Kit.git",
    "directory": "packages/cli"
  },
  "author": "jwiedeman",
  "keywords": [
    "gtm",
    "google-tag-manager",
    "cli",
    "setup"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "bin": {
    "gtm-kit": "./dist/cli.js",
    "gtm-kit-init": "./dist/cli.js"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "lint": "eslint --max-warnings=0 \"src/**/*.{ts,tsx}\"",
    "test": "jest --config ./jest.config.cjs --runInBand",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {},
  "devDependencies": {
    "tslib": "^2.6.2"
  }
}
